    <name value="MaternalInfantHealthResearch"/>
    <title value="Longitudinal Maternal &amp; Infant Health Information for Research"/>
    <status value="draft"/>
    <experimental value="false"/>
    <date value="2021-06-28"/>
    <publisher value="Lantana Consulting Group"/>
    <contact>
